{
  "name": "smartclide-deployment-extension",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/output": "1.19.0",
    "@theia/git": "1.19.0",
    "@types/js-yaml": "^4.0.4"
  },
  "devDependencies": {
    "rimraf": "latest",
    "typescript": "latest"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/smartclide-deployment-extension-frontend-module",
      "backend": "lib/node/smartclide-deployment-extension-backend-module"
    }
  ]
}