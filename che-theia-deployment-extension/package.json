{
  "name": "che-theia-deployment-extension",
  "version": "0.0.1",
  "license": "MIT",
  "keywords": [
    "theia-widget"
  ],
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "1.18.0-next.120",
    "@theia/git": "1.18.0-next.120",
    "@theia/output": "1.18.0-next.120",
    "recharts": "2.1.6"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/che-theia-deployment-extension-module",
      "backend": "lib/node/che-theia-deployment-backend-module"
    }
  ]
}