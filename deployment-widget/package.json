{
  "name": "smartclide-deployment-widget",
  "version": "0.0.0",
  "keywords": [
    "theia-widget"
  ],
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "1.19.0",
    "@theia/git": "1.19.0",
    "@theia/output": "1.19.0",
    "@types/node": "16.11.9",
    "recharts": "2.1.6"
  },
  "devDependencies": {
    "@theia/plugin-packager": "latest",
    "css-loader": "^6.7.1",
    "jest": "26.6.3",
    "rimraf": "latest",
    "ts-node": "9.1.1",
    "typescript": "latest"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "build": "tsc && theia-plugin pack"
  },
  "engines": {
    "theiaPlugin": "next"
  },
  "theiaPlugin": {
    "frontend": "lib/browser/widget-frontend-module.js",
    "backend": "lib/node/backend-module.js"
  }
}